        <script>
            $(document).ready(function () {
                const I = require('./node_modules/immutable');
                const data = require('../../data.json');

                const iData = I.List(data);



                function SaveJsonFile(data) {
                    $.ajax({
                        type: 'POST',
                        dataType: 'json',
                        async: false,
                        data: JSON.stringify(data),
                        url: '/api/IpAddress',
                        contentType: 'application/json; charset=utf-8',
                        success: function (result) {
                            if (result == true) {
                                debugger;
                                console.log('okok');
                            }
                            else {
                                debugger;
                                console.log('Error');
                                //popupNotification.show("Error when using API" + $('#txtAPI').val());
                            }
                        },
                        error: function (result) {
                            console.log(result);
                            popupNotification.show("Error when using API");
                        }
                    });

                }

                //var popupNotification = $("#popupNotification").kendoNotification().data("kendoNotification");
                $('.btn-get').click(function () {
                    debugger;
                    if ($('#txtAPI').val() === "") {
                        return;
                    }
                    $.ajax({
                        type: 'GET',
                        dataType: 'json',
                        async: false,
                        url: 'https://' + $('#txtAPI').val() + '/api/informations',
                        contentType: 'application/json; charset=utf-8',
                        success: function (result) {
                            debugger;
                            if (Object.keys(result).length > 1) {
                                debugger;
                                console.log('Final');
                                SaveJsonFile($('#txtAPI').val());
                                window.location.href = "/Home/Login";
                            }
                            else {
                                console.log('Error');
                                popupNotification.show("Error when using API" + $('#txtAPI').val());
                            }
                        },
                        error: function (result) {
                            console.log(result);
                            popupNotification.show("Error when using API");
                        }
                    });
                })
            });
        </script>
